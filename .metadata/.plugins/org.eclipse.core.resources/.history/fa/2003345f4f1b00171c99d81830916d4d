package baekjoon;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		int N = sc.nextInt();

		if (N >= 1 && N <= 9) {
			for (int i = 1; i < 10; i++) {
				System.out.println(N + " * " + i + " = " + (N * i));
			}
		}
		else
			System.out.println("입력범위를 초과하였습니다.");
		sc.close();
	}

}
