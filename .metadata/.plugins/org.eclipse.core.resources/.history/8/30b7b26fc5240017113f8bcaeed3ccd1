package baekjoon;

import java.util.Scanner;

public class Main {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		String input = sc.next();
		char cmp;
		int[] out = new int[26];

		for (int i = 0; i < out.length; i++) {
			out[i] = -1;
		}

		for (int i = 0; i < input.length(); i++) {
			cmp = input.charAt(i);
			for (int j = 0; j < 26; j++) {
				if (cmp == (j + 97) && out[j]==-1) {
					out[j] = i;
					break;
				}
			}
		}

		for (int i = 0; i < 26; i++)
			System.out.print(out[i] + " ");

		sc.close();
		
	}
}
