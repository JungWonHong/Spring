package report_no6;

public class multi_table {

	public static void main(String[] args) {
		int multi[][] = new int[9][];
		
		for(int i = 1;i<10;i++){
			System.out.println(i+"´Ü");
			
			for(int j = 1;j<10;j++){
				multi[i][j] = i*j;
				System.out.print(i+" x "+j+"="+multi[i][j]+" ");
			}
			System.out.println();
		}
	}

}
