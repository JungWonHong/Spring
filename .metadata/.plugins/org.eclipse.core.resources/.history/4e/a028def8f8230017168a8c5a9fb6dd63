package report_0418;

public class Lotto {

	public static void main(String[] args) {

		int[] lotto = new int[6];
		for (int i = 0; i < lotto.length; i++) {
			int num = (int) (Math.random() * 45 + 1);
			for (int j = 0; j < i + 1;) {
				if (num == lotto[j]) {
					num = (int) (Math.random() * 45 + 1);
				} else {
					j++;
				}
			}
			lotto[i] = num;
		}

		for (int i = 0; i < lotto.length; i++)
			System.out.print(lotto[i] + "\t");

	}

}
