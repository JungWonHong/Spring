package report_no6;

public class multi_table {

	public static void main(String[] args) {
		int multi[][] = new int[9][9];

		for (int i = 0; i < multi.length+1; i++) {
			for (int j = 0; j < multi[i].length; j++) {
				if (i == 0) {
					System.out.print((j + 1) + "´Ü\t\t");
					
					continue;
				}

				multi[i][j] = i * (j + 1);
				System.out.print((j + 1) + " x " + i + " = " + multi[i][j] + "\t");
			}
			System.out.print("\n");

		}
	}

}
