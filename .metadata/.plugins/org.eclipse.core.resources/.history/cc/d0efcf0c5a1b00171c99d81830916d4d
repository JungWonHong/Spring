package baekjoon;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();

		if (n >= 1 && n <= 10000) {
			int sum = 0;
			for(int i=1;i<=n;i++)
				sum+=i;
			System.out.println(sum);
		}
		else
			System.out.println("입력값 범위초과");
		
		sc.close();
	}

}
