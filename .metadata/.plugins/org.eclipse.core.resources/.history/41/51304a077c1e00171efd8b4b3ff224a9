package report_0411;

public class BPhone implements IFunction {
	String name;
	
	BPhone(String name){
		this.name = name;
	}
	
	public void callSenderReceive() {
		System.out.println("전화 송/수신 가능합니다.");
	}

	public void canLte() {
		System.out.println("4G입니다.");
	}

	public void tvRemoteController() {
		System.out.println("TV 리모콘 탑재 되었습니다.");
	}

}
