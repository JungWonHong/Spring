package report_no6;

public class multi_table {

	public static void main(String[] args) {
		int multi[][] = new int[9][9];
		
		for(int i = 1;i<multi.length+1;i++){
			for(int j = 1;j<multi[i].length+1;j++){
				multi[i-1][j-1] = i*j;
				System.out.print(j+" x "+i+" = "+multi[i-1][j-1]+"\t");
			}
			System.out.print("\n");
				
		}
	}

}
