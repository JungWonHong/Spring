package baekjoon;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int N = sc.nextInt();
		
		if(N<1&&N>100)
			N = sc.nextInt();
		
		int line = sc.nextInt();
		int sum = 0;
		for(int i = N-1;i>=0;i--){
			sum += line/(int)Math.pow(10, i);
		}
		
		System.out.println(sum);
		
		sc.close();
	}

}
